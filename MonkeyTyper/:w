#include <stdio.h>
#include <string.h>
void readFile() {
  FILE *ftpr = fopen("./text.txt", "r");
  char mystring[100];
  char allText[100][100];
  int i = 0;
  while (fgets(mystring, 100, ftpr)) {
    strcpy(allText[i], mystring) i++;
  }
}
int main(int argc, char *argv[]) {
  readFile();
  return 0;
}
